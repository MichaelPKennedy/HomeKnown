{
  "host": "localhost",
  "port": 3030,
  "public": "./public/",
  "origins": [
    "http://localhost:3030",
    "http://localhost:3000"
  ],
  "paginate": {
    "default": 10,
    "max": 50
  },
  "sequelize": {
    "dialect": "mysql",
    "host": "DB_HOST",
    "database": "DB_NAME",
    "username": "DB_USER",
    "password": "DB_PASS",
    "port": "DB_PORT"
  },
  "authentication": {
    "secret": "SECRET_KEY",
    "entity": "user",
    "service": "users",
    "entityId": "user_id",
    "authStrategies": [
      "jwt",
      "local",
      "custom"
    ],
    "oauth": {
      "redirect": "http://localhost:3000",
      "google": {
        "clientID": "GOOGLE_CLIENT_ID",
        "clientSecret": "GOOGLE_CLIENT_SECRET",
        "successRedirect": "http://localhost:3000",
        "failureRedirect": "http://localhost:3000/login",
        "scope": ["profile", "openid", "email"],
        "strategy": "google",
        "callbackURL": "http://localhost:3000/auth/google/callback"
      }
    },
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "http://localhost:3000",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "usernameField": "login",
      "passwordField": "password"
    },
    "custom": {
      "usernameField": "login",
      "passwordField": "password"
    }
  }
}