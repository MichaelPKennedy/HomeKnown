    "@turf/turf": "^6.5.0",
    "@types/sequelize": "^4.28.15",
    "@uidotdev/usehooks": "^2.4.0",
    "axios": "^1.6.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "feathers-hooks-common": "^8.1.1",
